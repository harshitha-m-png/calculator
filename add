#include <stdio.h>
int add(num1,num2)
{
return num1+num2;
}

int main() {
    int num1, num2,res;

    // Ask the user for two numbers
    printf("Enter first number: ");
    scanf("%d", &num1);
    printf("Enter second number: ");
    scanf("%d", &num2);

  res=add(num1,num2);
    printf("The sum is: %d\n",res);

    return 0;
}
